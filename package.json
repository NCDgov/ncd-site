{
  "name": "ncd-site",
  "version": "1.0.1",
  "description": "Temporary USWDS-backed landing page built with Jekyll.",
  "main": "postcss.config.js",
  "scripts": {
    "uswds:copy": "node scripts/copy-uswds.js",
    "build": "npm run uswds:copy && bundle exec jekyll build",
    "develop": "npm run uswds:copy && bundle exec jekyll serve --livereload",
    "pages": "npm run uswds:copy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NCDgov/ncd-site.git"
  },
  "keywords": [
    "jekyll",
    "uswds",
    "landing-page"
  ],
  "author": "Temporary Site Team",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/NCDgov/ncd-site/issues"
  },
  "homepage": "https://github.com/NCDgov/ncd-site#readme",
  "dependencies": {
    "uswds": "^2.14.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "postcss-cli": "^11.0.0"
  }
}
